                                <script type="text/javascript">
                                    var productDescriptInfo = {
                                        originalSize: 0,
                                        changeSize: 5000,
                                        onClickMore: function() {
                                            jQuery("#productMore_more").hide();
                                            jQuery("#productMore_close").show();
                                            jQuery("#prdDescIfrm").height(this.originalSize);
                                            event.preventDefault();
                                            event.stopPropagation();
                                        },
                                        onClickClose: function() {
                                            jQuery("#productMore_more").show();
                                            jQuery("#productMore_close").hide();
                                            jQuery("#prdDescIfrm").height(this.changeSize);
                                            var offset = jQuery("#productMore").offset();
                                            $('body,html').animate({
                                                    scrollTop: offset.top
                                                },
                                                500);
                                        },
                                        prdDescLoad: function() {
                                            var type = "null";
                                            switch (type) {
                                                case "b":
                                                    var isSmartOption = "Y";
                                                    if (isSmartOption === 'Y') {
                                                        break;
                                                    }
                                                    var iframe = jQuery("#prdDescIfrm");
                                                    if (this.changeSize === 0 || iframe.height() <= this.changeSize) {
                                                        jQuery("#productMore_more").hide();
                                                    } else {
                                                        this.originalSize = iframe.height();
                                                        iframe.height(this.changeSize);
                                                        jQuery("#productMore_more").show();
                                                    }
                                                    break;
                                                default:
                                                    jQuery("#productMore_more").hide();
                                                    jQuery("#productMore_close").hide();
                                                    break;
                                            }
                                        }
                                    };
                                    jQuery("#prdDescIfrm").load(function() {
                                        productDescriptInfo.prdDescLoad();
                                    });
                                </script>
