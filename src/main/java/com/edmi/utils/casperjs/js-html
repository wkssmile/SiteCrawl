var RakeLoadScript = function() {
    var path = '//www.11st.co.kr/js/rake/rakeLog.js?v=20170808';
    try {
        var element = document.createElement('script');
        element.src = path;
        element.charset = 'euc-kr';
        document.head.appendChild(element);
    } catch(e) {}
}
</script>
