            <script type="text/javascript">
                if (typeof window.Combine === "undefined") {
                    window.CombineSearch = {};
                }
                var getChannel = function(areaCode) {
                    var areaCodes = [];
                    areaCodes['category'] = {
                        ch: '11st.ch2'
                    };
                    areaCodes['prd'] = {
                        ch: '11st.ch3'
                    };
                    areaCodes['title'] = {
                        ch: '11st.ch5'
                    };
                    var channel = areaCodes[areaCode];
                    if (channel) {
                        return channel;
                    } else {
                        return areaCodes['category'];
                    }
                }
                CombineSearch.PowerLink = {
                    baseRequestUrl: 'http://ext.apbox.nbp-corp.com/11st/adunit',
                    dataSize: 0,
                    content: '',
                    requestData: function(config) {
                        var me = this;
                        try {
                            jQuery.ajax({
                                url: me.createRequestUrl(config),
                                dataType: "jsonp",
                                scriptCharset: "utf-8",
                                success: function(data) {
                                    me.contData = data.ads;
                                    me.dataSize = me.contData.length;
                                    var hyphen = "- ";
                                    if (me.dataSize > 0) {
                                        jQuery('#idPowerLink').show();
                                        for (var i = 0; i < me.dataSize && i < 6; i++) {
                                            var dataList = me.contData[i];
                                            var title = dataList.title;
                                            var desc = hyphen.concat(dataList.desc);
                                            var clickUrl = dataList.cUrl;
                                            var vUrl = dataList.vUrl;
                                            title = title.replace('탄력크림', '<em>탄력크림<\/em>');
                                            desc = desc.replace('탄력크림', '<em>탄력크림<\/em>');
                                            jQuery('#idPowerLinkTitle' + i).attr('href', clickUrl);
                                            jQuery('#idPowerLinkTitle' + i + ' > strong').html(title);
                                            jQuery('#idPowerLinkTitle' + i + ' > em').html(desc);
                                            jQuery('#idPowerLinkTitle' + i + ' > span').text(vUrl);
                                            jQuery('#idPowerLinkLi' + i).show();
                                        }
                                    }
                                },
                                error: function() {}
                            });
                        } catch(e) {}
                    },
                    createRequestUrl: function(config) {
                        var me = this;
                        var url = ['output=jsonp']; //url.push('ch=11st.ch1');url.push('ch=' +  getChannel( config.areaCode ).ch );url.push('kwd1='+encodeURIComponent(" 탄력크림"));url.push('cnt=6');url.push('ref='+encodeURIComponent(document.referrer));url.push('cat1='+encodeURIComponent(" 스킨케�?"));url.push('cat2='+encodeURIComponent(" 탄력크림 "));url.push('cat3='+encodeURIComponent(" 탄력크림"));url.push('cat4='+encodeURIComponent(""));return me.baseRequestUrl + '?' + url.join('&');}};jQuery(document).ready(function() {CombineSearch.PowerLink.requestData({areaCode : 'prd', baseRequestUrl : 'http://ext.apbox.nbp-corp.com/11st/adunit', contTitle : '', cat1 : '스킨케�?, cat2 : '탄력크림', cat3 : '탄력크림', cat4 : ''});});

            </script>
