            <script type="text/javascript">
                < !--
                var popCnt = 0;
                function getAdPopup(dispSpceId, dispAdvrtNo, dispObjNo, dispCtgrNo, relRelObjectNo, width, height, popCnt, isQueryCache, isDbCache) {
                    try {
                        if (null == dispCtgrNo || dispCtgrNo == undefined) {
                            dispCtgrNo = 0;
                        }
                        if (null == width || width == undefined) {
                            width = 400;
                        }
                        if (null == height || height == undefined) {
                            height = 450;
                        }
                        if (null == popCnt || popCnt == undefined) {
                            popCnt = 0;
                        }
                        if ('' != dispSpceId && '' != dispAdvrtNo && '' != dispObjNo) {
                            var popId = "PARTNER_COUPON_POPUP_" + dispObjNo;
                            var params = "<form name='frmPop_" + (++popCnt) + "' method='post'>" + "<input name='dispSpceId' value='" + dispSpceId + "'/>" + "<input name='dispAdvrtNo' value='" + dispAdvrtNo + "'/>" + "<input name='dispObjNo'  value='" + dispObjNo + "'/>" + "<input name='dispCtgrNo' value='" + dispCtgrNo + "'/>" + "<input name='relRelObjectNo'  value='" + relRelObjectNo + "'/>" + "<input name='isQueryCache' value='" + isQueryCache + "'/>" + "<input name='isDbCache' value='" + isDbCache + "'/>" + "<input name='popId' value='" + popId + "'/>" + "</form>";
                            document.getElementById('hideForm').innerHTML = params;
                            if (! (TMCookieUtil.isExist(1, popId) || TMCookieUtil.isExist(4, popId))) {
                                var win = window.open("", popId, "scrollbars=no, width=" + width + "px, height=" + height + "px");
                                if (win || win.focus()) {
                                    var frm = eval('document.frmPop_' + popCnt);
                                    if (frm) {
                                        frm.action = "http://www.11st.co.kr/product/SellerProductDetail.tmall?method=popSellerProductDetailAd";
                                        frm.target = popId;
                                        frm.submit();
                                        win.focus();
                                    }
                                }
                            }
                        }
                    } catch(e) {}
                }-->
            </script>
