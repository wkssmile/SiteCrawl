<script>
    if (document.addEventListener) {
        document.addEventListener("DOMContentLoaded", RakeLoadScript);
    } else {
        document.attachEvent("onreadystatechange",
            function() {
                if (document.readyState !== "loading") {
                    RakeLoadScript();
                }
            });
    }
</script>
